class Solution {
public:
    vector<int> findDuplicates(vector<int>& nums) {
        unordered_map<int, int> mp;
        vector<int> ans;
        for(int i=0; i<nums.size(); i++) {
            mp[nums[i]]++;
        }

        for(auto itr: mp) {
            if(itr.second == 2) {
                ans.push_back(itr.first);
            }
        }
        return ans;
    }
};
